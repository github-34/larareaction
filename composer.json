{
    "name": "insomnicles/laraexpress",
    "description": "A Laravel package for reactions, ratings, emoticons, and other custom expression types",
    "license": "MIT",
    "authors": [
        {
            "name": "Author Name",
            "email": "author@email.com",
            "homepage": "http://author.com"
        }
    ],
    "homepage": "https://github.com/insomnicles/laraexpress",
    "keywords": ["Laravel", "Laraexpress"],
    "require": {
        "illuminate/support": "~9"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~9"
    },
    "autoload": {
        "psr-4": {
            "Insomnicles\\Laraexpress\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Insomnicles\\Laraexpress\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Insomnicles\\Laraexpress\\LaraexpressServiceProvider"
            ],
            "aliases": {
                "Laraexpress": "Insomnicles\\Laraexpress\\Facades\\Laraexpress"
            }
        }
    }
}
